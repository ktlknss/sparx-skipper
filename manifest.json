{
    "manifest_version": 2,
    "name": "Sparx Skipper",
    "version": "1.0",

    "description": "Skips sparx questions when you click the icon",

    "icons": {
        "48": "icons/icon-48.png"
    },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["content.js"]
        }
    ],

    "permissions": ["activeTab"]
}
